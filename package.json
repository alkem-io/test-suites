{
  "name": "@alkemio/test-suites",
  "version": "0.1.0",
  "description": "Alkemio system level API / integration tests",
  "author": "Cherrytwist Foundation",
  "private": false,
  "license": "EUPL-1.2",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkemio/test-suites.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint:prod": "tsc --noEmit && cross-env NODE_ENV=production eslint src/**/*.ts{,x}",
    "lint:fix": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
    "codegen": "graphql-codegen --config codegen.ts",
    "test": "jest",
    "test:ui": "jest --detectOpenHandles --runInBand --config ./test/config/jest.config.ts",
    "stop-process": "fuser 4000/tcp -k",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --forceExit --maxWorkers=3  --config ./test/config/jest.config.e2e.js",
    "test:it": "jest --forceExit --maxWorkers=4  --config ./test/config/jest.config.it.js",
    "test:auth": "jest --detectOpenHandles --runInBand --config ./test/config/jest.config.auth.js",
    "test:auth-entities": "jest ---forceExit --maxWorkers=4 --config ./test/config/jest.config.auth-entities.js",
    "test:auth-global-roles": "jest ---forceExit --maxWorkers=4 --config ./test/config/jest.config.auth-global-roles.js",
    "test:ut": "jest --detectOpenHandles --runInBand --config ./test/config/jest.config.js",
    "test:nightly": "jest  --maxWorkers=6 --config ./test/config/jest.config.nightly.js",
    "test:ci": "jest --forceExit --runInBand --config ./test/config/jest.config.ci.js",
    "test:communications": "jest --forceExit --runInBand --config ./test/config/jest.config.communications.js",
    "test:notifications": "jest --forceExit --runInBand --config ./test/config/jest.config.notifications.js",
    "test:integration-parallel": "jest --forceExit --maxWorkers=4 --config ./test/config/jest.config.integration.js",
    "test:organization": "jest --forceExit --maxWorkers=6 --config ./test/config/jest.config.organization.js",
    "test:search": "jest --forceExit --maxWorkers=6 --config ./test/config/jest.config.search.js",
    "test:communication": "jest --forceExit --runInBand --config ./test/config/jest.config.communication.js",
    "test:journey": "jest --forceExit --maxWorkers=6 --config ./test/config/jest.config.journey.js",
    "test:activity-logs": "jest --forceExit --maxWorkers=6 --config ./test/config/jest.config.activity-logs.js",
    "test:usermanagement": "jest --forceExit --maxWorkers=4 --config ./test/config/jest.config.usermanagement.js",
    "test:integration": "jest --forceExit --maxWorkers=1 --verbose --config ./test/config/jest.config.integration.js",
    "test:upload": "jest --forceExit --maxWorkers=1 --verbose --config ./test/config/jest.config.upload.js",
    "test:preferences": "jest --forceExit --maxWorkers=3 --config ./test/config/jest.config.preferences.js",
    "test:pagination": "jest --forceExit --maxWorkers=1 --verbose --config ./test/config/jest.config.pagination.js",
    "test:roles": "jest --forceExit --maxWorkers=1 --verbose --config ./test/config/jest.config.roles.js",
    "test:subscriptions": "jest --forceExit --maxWorkers=1 --verbose --config ./test/config/jest.config.subscriptions.js",
    "test:conversion": "jest --forceExit --maxWorkers=1 --verbose --config ./test/config/jest.config.conversion.js",
    "test:templates": "jest --forceExit --maxWorkers=1 --verbose --config ./test/config/jest.config.templates.js",
    "test:innovation": "jest --forceExit --maxWorkers=1 --verbose --config ./test/config/jest.config.innovationPacks.js",
    "test:documents": "jest --forceExit --maxWorkers=1 --verbose --config ./test/config/jest.config.documents.js",
    "innovation-pack-with-templates": "ts-node-dev ./test/functional-api/innovation-pack/innovation-pack-with-templates.ts",
    "test:orphaned": "jest --forceExit --maxWorkers=1 --verbose --config ./test/config/jest.config.orphaned.js",
    "register-user": "ts-node-dev ./test/utils/register-user.ts"
  },
  "dependencies": {
    "@alkemio/client-lib": "^0.26.0",
    "@ory/kratos-client": "^0.11.1",
    "@types/express-session": "^1.17.3",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "express-session": "^1.17.1",
    "fs": "^0.0.1-security",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^5.8.2",
    "node-fetch": "^3.3.1",
    "ws": "^8.7.0",
    "graphql-upload": "^16.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@graphql-codegen/import-types-preset": "^2.2.3",
    "@graphql-codegen/typed-document-node": "^2.3.3",
    "@graphql-codegen/typescript-graphql-request": "^4.5.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@types/cache-manager": "^3.4.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.0",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^16.4.6",
    "@types/puppeteer": "^5.4.4",
    "@types/supertest": "^2.0.11",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "coveralls": "^3.1.0",
    "dotenv": "^16.0.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.3.1",
    "graphql-request": "^3.3.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "lint-staged": "^10.5.4",
    "prettier": "^1.19.1",
    "puppeteer": "^13.3.2",
    "start-server-and-test": "^1.12.6",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.7",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.5"
  },
  "_moduleAliases": {
    "@src": "./dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8.5.5"
  }
}
